                                       <option value="2">Kontrolni pregled</option>
                                       <option value="121">Koznultacije</option>
                                       <option value="3">Terapijski postupci</option>
                                       <option value="4">Dijagnostika</option>
                                       <option value="61">Nacionalni preventni program</option>
                                       <option value="101">Dnevna bolnica</option>
                                    </select>
                                 </div>
                                 <div id="9" class="stanje9">
                                    <p style="margin:15px 0"> Unesite ustanovu: </p>
                                    <input id = "sel_stanja9" name="itemstanje9" class = "typeahead tt query" type="text">
                                 </div>
                                 <button value="submit" type="submit" style="width:auto;display:block;margin:0 auto;cursor:pointer"> <img src="http://findicons.com/files/icons/977/rrze/32/magnifying_glass.png"></img></button>
                              </div>
                           </form>
                        </div>
                        <div class="modal-footer">
                           <button type="button" class="btn btn-default" data-dismiss="modal"> Zatvori </button>
                        </div>
